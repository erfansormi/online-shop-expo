{
  "expo": {
    "scheme": "online-shop-expo",
    "name": "Bahman Kala",
    "slug": "online-shop-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f42c45"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.erfansormi.onlineshopexpo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ],
      "expo-font",
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      "expo-build-properties"
    ],
    "extra": {
      "supportsRTL": true,
      "forcesRTL": true,
      "eas": {
        "projectId": "63016323-4252-4569-9d2a-7e8c6747b488"
      }
    }
  },
  "experiments": {
    "tsconfigPaths": true
  }
}